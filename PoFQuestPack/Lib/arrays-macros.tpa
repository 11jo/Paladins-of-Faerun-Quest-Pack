// WED in AREAS
DEFINE_PATCH_MACRO UPDATE_POF_WED_NAMES_ON_AREA BEGIN
  READ_LONG  0x8 wed_off ELSE 0
//  READ_LONG  0x8 wed_off ELSE 0
  PATCH_PRINT ~PRE-LOOP~
  FOR (index = 0; index < wed_num; ++index) BEGIN
    SET currentwed = wed_off + index * 0x8
    READ_ASCII currentwed currentwed_name (8) NULL
    TO_UPPER currentwed_name
    PATCH_PRINT ~INDEX: %index% NAME: %currentwed_name%~
    PATCH_IF (VARIABLE_IS_SET $wed_filenames_to_change(~currentwed_name~)) BEGIN
      SPRINT currentwed_newname $wed_filenames_to_change(~currentwed_name~)
      PATCH_PRINT ~INDEX: %index% NAME: %currentwed_name% NEWNAME: %currentwed_newname%~
      WRITE_ASCII currentwed currentwed_newname (8)
    END
  END
END

