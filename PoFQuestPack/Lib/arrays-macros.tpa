// Items in STORES
DEFINE_PATCH_MACRO UPDATE_POF_ITEM_NAMES_ON_STORE BEGIN
  READ_LONG  0x34 itm_off ELSE 0
  READ_LONG  0x38 itm_num ELSE 0
  PATCH_PRINT ~PRE-LOOP~
  FOR (index = 0; index < itm_num; ++index) BEGIN
    SET currentitem = itm_off + index * 0x1c
    READ_ASCII currentitem currentitem_name (8) NULL
    TO_UPPER currentitem_name
    PATCH_PRINT ~INDEX: %index% NAME: %currentitem_name%~
    PATCH_IF (VARIABLE_IS_SET $item_filenames_to_change(~%currentitem_name%~)) BEGIN
      SPRINT currentitem_newname $item_filenames_to_change(~%currentitem_name%~)
      PATCH_PRINT ~INDEX: %index% NAME: %currentitem_name% NEWNAME: %currentitem_newname%~
      WRITE_ASCIIE currentitem ~%currentitem_newname%~ (8)
    END
  END
END

