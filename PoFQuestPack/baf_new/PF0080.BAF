IF
  Global("DEADW","PF0080",0)
  Dead("DRE1MOD")  // ~Прислуга Элистрая~
  Dead("DRE2MOD")  // ~Прислуга Элистрая~
  Dead("DRE3MOD")  // ~Прислуга Элистрая~
  Dead("DRE4MOD")  // ~Прислуга Элистрая~
THEN
  RESPONSE #100
    SetGlobal("DEADW","PF0080",1)
    ActionOverride("DWQMOD",StartDialogue("DWQ1MOD",Player1))
END

