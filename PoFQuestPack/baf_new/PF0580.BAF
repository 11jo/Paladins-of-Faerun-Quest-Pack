IF
  OnCreation()
  Global("EnteredAreaPF0580","PF0580",0)
THEN
  RESPONSE #100
    SetGlobal("EnteredAreaPF0580","PF0580",1)
    ClearAllActions()
    HideGUI()
    StartCutSceneMode()
    DisplayStringHead("DRLUMOD",@0) 
    Wait(5)
    ActionOverride("DRLUMOD",Attack(Player1))
    DisplayStringHead(Player2,@1)  
    Wait(4)
    DisplayStringHead(Player3,@2)  
    Wait(3)
    ScreenShake([20.20],3)
    ActionOverride("DREUMOD",Attack("DRLUMOD"))
    Wait(5)
    DisplayStringHead("DRLUMOD",@3)  
    Wait(2)
    ActionOverride("DREUMOD",Kill("DRLUMOD"))
    Wait(1)
    DisplayStringHead("DREUMOD",@4)  
    Wait(5)
    AddJournalEntry(@5,QUEST)
    ActionOverride(Player2,LeaveAreaLUA("PFA480","",[403.562],0))
    ActionOverride(Player1,LeaveAreaLUA("PFA480","",[429.599],0))
    ActionOverride(Player3,LeaveAreaLUA("PFA480","",[483.561],0))
    ActionOverride(Player4,LeaveAreaLUA("PFA480","",[407.505],0))
    ActionOverride(Player5,LeaveAreaLUA("PFA480","",[479.463],0))
    ActionOverride(Player6,LeaveAreaLUA("PFA480","",[530.521],0))
    UnhideGUI()
    EndCutSceneMode()
END

