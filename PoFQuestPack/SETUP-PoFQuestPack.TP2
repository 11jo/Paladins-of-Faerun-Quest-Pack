	  COPY ~PoFQuestPack/Languages/%LANGUAGE%/Baf_new~			~weidu_external/PoFQuestPack/Languages/autotra/%LANGUAGE%/Baf_new~
	  COPY ~PoFQuestPack/Languages/%LANGUAGE%/Baf_are~			~weidu_external/PoFQuestPack/Languages/autotra/%LANGUAGE%/Baf_are~
	  COPY ~PoFQuestPack/Languages/%LANGUAGE%/D~				~weidu_external/PoFQuestPack/Languages/autotra/%LANGUAGE%/D~
      COPY ~PoFQuestPack/Languages/%LANGUAGE%~					~weidu_external/PoFQuestPack/Languages/autotra/%LANGUAGE%~
